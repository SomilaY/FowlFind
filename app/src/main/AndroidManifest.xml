<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >


    <permission
        android:name="com.FowlFind.googlemaps.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />


    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/fowlfindlogo"
        android:label="Fowl Find"
        android:supportsRtl="true"
        android:theme="@style/Theme.Googlemaps"
        tools:targetApi="31" >

        <!--
             TODO: Before you run your application, you need a Google Maps API key.

             To get one, follow the directions here:

                https://developers.google.com/maps/documentation/android-sdk/get-api-key

             Once you have your API key (it starts with "AIza"), define a new property in your
             project's local.properties file (e.g. MAPS_API_KEY=Aiza...), and replace the
             "YOUR_API_KEY" string in this file with "${MAPS_API_KEY}".
        -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyD43GOABtBgoNFx-b2uCk50Hnf6vfhrAi8" />

        <activity
            android:name=".MapsActivity"
            android:exported="true"
            android:label="@string/title_activity_maps">

        </activity>


        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ListObservations"
            android:exported="true"/>

        <activity android:name=".Howto"
            android:exported="true"/>

        <activity android:name=".BirdTips"
            android:exported="true"/>

        <activity android:name=".PastObservationsActivity"
            android:exported="true"/>

        <activity android:name=".Community"
            android:exported="true"/>

        <activity android:name=".SettingsActivity"
            android:exported="true" />

        <activity android:name=".info"
        android:exported="true" />

        <activity android:name=".signin"
            android:exported="true" />
    </application>


</manifest>